{
    "name": "nue-extensions/settings",
    "description": "Additional tools to help you in development with Nue.",
    "type": "library",
    "keywords": ["nue", "extension"],
    "homepage": "https://github.com/nue-extensions/settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Noviyanto Rahmadi",
            "email": "novay@btekno.id"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "novay/nue": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Nue\\Setting\\": "src/"
        }
    },
    "extra": {
